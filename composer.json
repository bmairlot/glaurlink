{
    "name": "ancalagon/glaurlink",
    "description": "Glaurlink project",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ancalagon\\Glaurlink\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ancalagon\\Glaurlink\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Your Name",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}